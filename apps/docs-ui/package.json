{
  "name": "@xteam-app/docs-ui",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "doc": "build-storybook --docs -o ./dist",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "dev": "start-storybook -p 6006 --quiet",
    "dev:native:android": "expo start --android --localhost",
    "dev:native:ios": "expo start --ios --localhost",
    "dev:android": "run-p update-stories dev:native:android",
    "dev:ios": "run-p update-stories dev:native:ios",
    "lint": "eslint"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "4.0.0",
    "@react-native-community/slider": "4.1.12",
    "@storybook/addon-actions": "~6.3",
    "@storybook/addon-controls": "~6.3",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "@xteam-app/config": "*",
    "@xteam-app/ui": "*",
    "expo": "~44.0.0",
    "expo-status-bar": "~1.2.0",
    "react": "17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "0.64.3",
    "react-native-web": "^0.17.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@storybook/addon-essentials": "~6.3",
    "@storybook/addon-links": "~6.3",
    "@storybook/addon-react-native-web": "^0.0.18",
    "@storybook/react": "~6.3",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@xteam-app/eslint-config": "^1.0.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-native-web": "^0.17.5",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "~4.3.5"
  }
}
