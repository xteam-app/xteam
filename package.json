{
  "name": "@xteam-app/xteam",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start:ios": "npm run ios -w @xteam-app/app",
    "start:web": "npm run web -w @xteam-app/app",
    "docs:ui:ios": "npm run start:ios -w @xteam-app/docs-ui",
    "docs:ui:web": "npm run start:web -w @xteam-app/docs-ui",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "reset": "find . -type dir -name node_modules | xargs rm -rf && rm -rf package-lock.json"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.5.0"
}