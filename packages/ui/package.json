{
  "name": "@xteam-app/ui",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "react-native": "src/index.tsx",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn  -o ./src/theme/tailwind.json",
    "build:bob": "bob build",
    "build": "run-s build:tailwind build:bob",
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn  -o ./src/theme/tailwind.json --watch\"",
    "dev:ios": "npm run dev",
    "dev:android": "npm run dev",
    "lint": "eslint"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@xteam-app/config": "*",
    "@xteam-app/eslint-config": "*",
    "concurrently": "^7.1.0",
    "eslint": "^8.15.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.13",
    "react-native-builder-bob": "^0.18.2",
    "tailwindcss": "^3.0.24",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  }
}
