{
  "name": "@xteam-app/ui",
  "version": "0.0.0",
  "license": "MIT",
  "main": "src/index.tsx",
  "commonjs": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "source": "src/index.ts",
  "react-native": "src/index.tsx",
  "types": "src/index.ts",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "build:tailwind": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn  -o ./src/theme/tailwind.json",
    "dev": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn  -o ./src/theme/tailwind.json --watch\"",
    "dev:ios": "npm run dev",
    "dev:android": "npm run dev"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "concurrently": "^7.1.0",
    "config": "*",
    "postcss": "^8.4.13",
    "react-native-builder-bob": "^0.18.2",
    "tailwindcss": "^3.0.24",
    "tsconfig": "*",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "tailwind-rn": "^4.2.0"
  }
}
